<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">
    <Transition
        android:id="@+id/category_1_to_2"
        motion:constraintSetStart="@+id/category_set1"
        motion:constraintSetEnd="@+id/category_set2"
        motion:duration="500"
        motion:motionInterpolator="cubic(.8,0,.5,1)">
    </Transition>
    <Transition
        android:id="@+id/category_2_to_3"
        motion:constraintSetStart="@+id/category_set2"
        motion:constraintSetEnd="@+id/category_set3"
        motion:duration="500"
        motion:motionInterpolator="cubic(.8,0,.5,1)">
    </Transition>
    <ConstraintSet android:id="@+id/category_set1">

        <Constraint
            android:id="@+id/category_top_card"
            android:elevation="8dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0"
            motion:layout_constraintHeight_percent="0.35"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/category_top_graphic"
            android:elevation="8dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintTop_toTopOf="@id/category_top_card"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="@id/category_top_card"
            motion:layout_constraintVertical_bias="0.9"
            motion:layout_constraintHeight_percent="0.25"
            motion:layout_constraintHorizontal_bias="0.2"
            motion:layout_constraintDimensionRatio="1,1.35" />
        <Constraint
            android:id="@+id/category_scrollview"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="1.0"
            motion:layout_constraintTop_toBottomOf="@id/category_top_card"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/category_shopitems"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="1.0"
            motion:layout_constraintTop_toBottomOf="@id/category_top_card"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintStart_toEndOf="parent"
            motion:layout_constraintWidth_percent="0.99"/>
        <Constraint
            android:id="@+id/category_titlename"
            android:elevation="8dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="@id/category_top_graphic"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="@id/category_top_card"
            motion:layout_constraintVertical_bias="0"
            motion:layout_constraintHorizontal_bias="0.85" />
        <Constraint
            android:id="@+id/category_shopitem_details"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="1.0"
            motion:layout_constraintTop_toBottomOf="@id/category_top_card"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintStart_toEndOf="parent"
            motion:layout_constraintWidth_percent="0.99" />
        <Constraint
            android:id="@+id/category_shopitem_image"
            motion:layout_constraintWidth_percent="0.7"
            android:elevation="8dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toTopOf="@id/category_top_card"
            android:layout_marginBottom="10dp"
            motion:layout_constraintDimensionRatio="1,1"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/category_shopitem_name"
            android:elevation="8dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toTopOf="@id/category_shopitem_image"
            motion:layout_constraintVertical_bias="0" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/category_set2">

        <Constraint
            android:id="@+id/category_shopitems"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="1.0"
            motion:layout_constraintTop_toBottomOf="@id/category_top_card"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/category_top_card"
            android:elevation="8dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0"
            motion:layout_constraintHeight_percent="0.35"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/category_top_graphic"
            android:elevation="8dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintTop_toTopOf="@id/category_top_card"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="@id/category_top_card"
            motion:layout_constraintVertical_bias="0.9"
            motion:layout_constraintHeight_percent="0.25"
            motion:layout_constraintHorizontal_bias="0.2"
            motion:layout_constraintDimensionRatio="1,1.35" />
        <Constraint
            android:id="@+id/category_titlename"
            android:elevation="8dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="@id/category_top_graphic"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="@id/category_top_card"
            motion:layout_constraintVertical_bias="0"
            motion:layout_constraintHorizontal_bias="0.85" />
        <Constraint
            android:id="@+id/category_scrollview"
            motion:layout_constraintEnd_toStartOf="parent"
            motion:layout_constraintWidth_percent="0.99"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="1.0"
            motion:layout_constraintTop_toBottomOf="@id/category_top_card"
            motion:layout_constraintHorizontal_bias="0.0"/>
        <Constraint
            android:id="@+id/category_shopitem_details"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="1.0"
            motion:layout_constraintTop_toBottomOf="@id/category_top_card"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintStart_toEndOf="parent"
            motion:layout_constraintWidth_percent="0.99" />
        <Constraint
            android:id="@+id/category_shopitem_image"
            motion:layout_constraintWidth_percent="0.7"
            android:elevation="8dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toTopOf="@id/category_top_card"
            android:layout_marginBottom="10dp"
            motion:layout_constraintDimensionRatio="1,1"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/category_shopitem_name"
            android:elevation="8dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toTopOf="@id/category_shopitem_image"
            motion:layout_constraintVertical_bias="0" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/category_set3">
        <Constraint
            android:id="@+id/category_top_card"
            android:elevation="8dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0"
            motion:layout_constraintHeight_percent="0.45"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/category_top_graphic"
            android:elevation="8dp"
            motion:layout_constraintEnd_toStartOf="parent"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintTop_toTopOf="@id/category_top_card"
            motion:layout_constraintBottom_toBottomOf="@id/category_top_card"
            motion:layout_constraintVertical_bias="0.9"
            motion:layout_constraintHeight_percent="0.25"
            motion:layout_constraintHorizontal_bias="0.2"
            motion:layout_constraintDimensionRatio="1,1.35" />
        <Constraint
            android:id="@+id/category_titlename"
            android:elevation="8dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="@id/category_top_graphic"
            motion:layout_constraintStart_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="@id/category_top_card"
            motion:layout_constraintVertical_bias="0"
            motion:layout_constraintHorizontal_bias="0.85" />
        <Constraint
            android:id="@+id/category_scrollview"
            motion:layout_constraintEnd_toStartOf="parent"
            motion:layout_constraintWidth_percent="0.99"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="1.0"
            motion:layout_constraintTop_toBottomOf="@id/category_top_card"
            motion:layout_constraintHorizontal_bias="0.0"/>
        <Constraint
            android:id="@+id/category_shopitems"
            motion:layout_constraintEnd_toStartOf="parent"
            motion:layout_constraintWidth_percent="0.99"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="1.0"
            motion:layout_constraintTop_toBottomOf="@id/category_top_card"
            motion:layout_constraintHorizontal_bias="0.0" />
        <Constraint
            android:id="@+id/category_shopitem_details"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="1.0"
            motion:layout_constraintTop_toBottomOf="@id/category_top_card"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/category_shopitem_image"
            motion:layout_constraintWidth_percent="0.7"
            android:elevation="8dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintTop_toBottomOf="@id/category_shopitem_name"
            motion:layout_constraintBottom_toBottomOf="@id/category_top_card"
            motion:layout_constraintVertical_chainStyle="spread"
            motion:layout_constraintDimensionRatio="1,1"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/category_shopitem_name"
            android:elevation="8dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="@id/category_top_card"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toTopOf="@id/category_shopitem_image"
            motion:layout_constraintVertical_bias="0" />
    </ConstraintSet>
</MotionScene>